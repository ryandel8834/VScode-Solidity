{
	"name": "solidity",
	"description": "Ethereum Solidity Language for Visual Studio Code",
	"version": "0.0.3",
	"publisher": "JuanBlanco",
	"engines": {
		"vscode": "^0.10.1"
	},
	"icon": "images/icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/juanfranblanco/vscode-solidity"
	},
	"categories": [
		"Languages"
	],
	"contributes": {
		"languages": [{
			"id": "solidity",
			"aliases": ["Solidity", "solidity"],
			"extensions": [".sol"],
			"configuration": "./solidity.configuration.json"
		}],
       "snippets": [
       {
         "language": "solidity",
         "path": "./snippets/solidity.json"
        }
       ],
		"grammars": [{
			"language": "solidity",
			"scopeName": "source.solidity",
			"path": "./syntaxes/Solidity.tmLanguage"
		}]
	}
}
